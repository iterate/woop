{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "airbnb",
    "plugin:prettier/recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "env": {
    "browser": true
  },
  "rules": {
    "react/prop-types": "off"
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}